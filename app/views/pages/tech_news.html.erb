<main role="main" class="inner cover home-page">
  <h1 class="cover-heading">Rails News</h1>
  <p>#rubyonrails live feed tweets with URL parser.</p>
  <hr />
  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">            
            <%= twitter_parser tweet %>
          </div>          
        </div>        
      </div>
    <% end %>
  </div>
</main>
